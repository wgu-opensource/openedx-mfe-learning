{
  "name": "@wgu-opensource/openedx-mfe-learning",
  "version": "0.1.0",
  "description": "Learning Experience for OpenEDX",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wgu-opensource/openedx-mfe-learning.git"
  },
  "browserslist": [
    "extends @edx/browserslist-config"
  ],
  "scripts": {
    "build": "fedx-scripts webpack",
    "postinstall": "patch-package",
    "i18n_extract": "BABEL_ENV=i18n fedx-scripts babel src --quiet > /dev/null",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "lint:fix": "fedx-scripts eslint --fix --ext .js --ext .jsx .",
    "prepare": "husky install",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress",
    "test": "fedx-scripts jest --coverage --passWithNoTests"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "fedx-scripts eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "author": "WGU",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/wgu-opensource/openedx-mfe-learning#readme",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/wgu-opensource/openedx-mfe-learning/issues"
  },
  "dependencies": {
    "@edx/brand": "npm:@edx/brand-openedx@1.2.0",
    "@edx/frontend-app-learning": "github:wgu-opensource/frontend-app-learning#0.2.0",
    "@edx/frontend-component-footer": "12.1.2",
    "@edx/frontend-component-header": "4.2.3",
    "@edx/frontend-platform": "4.2.0",
    "@edx/paragon": "^20.20.0",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@reduxjs/toolkit": "1.8.1",
    "@testing-library/react": "10.4.9",
    "core-js": "3.31.1",
    "patch-package": "^8.0.0",
    "prop-types": "15.8.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-intl": "^5.25.0",
    "react-redux": "7.2.9",
    "react-responsive": "8.2.0",
    "react-router": "5.3.4",
    "react-router-dom": "5.3.4",
    "react-transition-group": "4.4.5",
    "redux": "4.2.1",
    "regenerator-runtime": "0.13.11",
    "reselect": "4.1.8",
    "classnames": "2.3.1"
  },
  "devDependencies": {
    "@edx/browserslist-config": "^1.1.1",
    "@edx/frontend-build": "12.9.2",
    "@edx/reactifex": "^2.1.1",
    "glob": "7.2.3",
    "husky": "7.0.4",
    "jest": "29.6.1",
    "lint-staged": "^14.0.1"
  }
}
